<h1> Personal Contact List</h1>

<%= link_to "Add new contact", "/contacts/new" %>

<% @names.each do |name| %>
<a href="/contacts/<%= name.id %>">
<h3> <%= name.name %> </h3></a>
<h3> <%= name.middle_name %> </h3>
<h3> <%= name.email_address %></h3>

 <h3> <%= name.mobile_number %></h3>
<h3> <%= name.bio %></h3>
<br>
<% end %>